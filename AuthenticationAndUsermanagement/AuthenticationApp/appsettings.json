{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "PLACEHOLDER"
  },
  "JWT": {
    "Audience": "http://localhost:7003",
    "Issuer": "http://localhost:7003",
    "SignInKey": "PLACEHOLDER"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": { "ServerUrl": "http://seq:80" }
      }
    ]
  }
}
